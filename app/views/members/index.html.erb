<%if @members.empty?%>
<span class="label label-warning">sorry no members found</span>
<%else%>
<span class="label label-success"><%= pluralize(@members.count, 'member') %> found</span>
<%end%>
<h3>All members</h3>

<%= render 'search'%>
<%= link_to image_tag("pdf.jpeg", size: "40x20", title: "all members"), members_path(format: :pdf), class: "span1 pull-right"%>



<%= render 'table'%>

<br />


<%= will_paginate @members%>
